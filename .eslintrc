{
  "parser": "babel-eslint",
  "plugins": [
  ],
  "settings": {
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": false,
      "blockBindings": true
    }
  },
  "rules": {
    "block-scoped-var": 2,
    "comma-dangle": [
      2,
      "never"
    ],
    "curly": 2,
    "dot-notation": 2,
    "eqeqeq": 2,
    "no-eq-null": 2,
    "no-undef": 2,
    "guard-for-in": 2,
    "new-cap": 2,
    "new-parens": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-negated-in-lhs": 2,
    "no-sparse-arrays": 2,
    "no-shadow": 2,
    "prefer-const": 2,
    "prefer-rest-params": 2,
    "prefer-spread": 2,
    "prefer-template": 2,
    "no-unexpected-multiline": 2,
    "semi": 2,
    "quotes": [
      2,
      "single"
    ],
    "use-isnan": 2,
    "no-var": 2,
    "no-delete-var": 2,
    "sort-imports": 2
  },
  "env": {
    "es6": true,
    "node": true
  }
}